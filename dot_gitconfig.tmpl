[user]
	name = {{ .git_name }}
	email = {{ .git_email }}
	signingkey = {{ .git_sign_key }}
[commit]
	gpgsign = true
[core]
	precomposeunicode = true
	quotepath = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[rebase]
	autoStash = true
